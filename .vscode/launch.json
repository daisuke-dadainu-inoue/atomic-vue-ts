  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceRoot}",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "restart": true,
      "port": 9229
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "processId": "${command:PickProcess}"
    }
  ]
}
